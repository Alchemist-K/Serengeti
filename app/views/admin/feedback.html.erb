<!--Main layout-->
<div class="container">
  <div class="row">
    <div class="col-md-12" style="margin: 30px 0 300px 0">
    <!--Main column-->
      <div class="col-md-8">
        <!--/.Card-->
        <% @feedback.each do |f| %>
          <div class="card">
            <!--Card content-->
            <div class="card-block">
              <p><strong>이름 : <%= f.username %></strong></p><hr>
              <p><%= f.feedback %></p>
            </div>
          </div> <!-- end card-block -->
        <% end %>
      </div> <!-- end card-->
      <div class="col-md-4">
        <div class="widget-wrapper">
            <h4><strong>관리자 페이지</strong></h4>
            <div class="list-group">
                <a href="/admin/index" class="list-group-item">세렝게티 현황</a>
                <a href="#" class="list-group-item active">피드백 내용</a>
                <a href="/admin/team" class="list-group-item">팀 현황</a>
                <a href="/admin/teambuild" class="list-group-item">팀빌딩 현황</a>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
