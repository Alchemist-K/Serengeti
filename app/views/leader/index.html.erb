<% unless current_user.nil? %>
  <%= current_user.id %><hr>
<% else %>
  <%= "no user" %>
<% end %>
<% @myTeam.each do |t| %>
  <% Teambuild.findWaitingTeambuild(t.id).each do |r| %>
    <%= r.user_id %>
    <%= r.team_id %>
    <%= form_tag("/mypage/leader/accept/#{r.id}", method: "GET") do %>
      <button class="btn btn-primary-outline waves-effect btn-size" type="submit">함께해요~</button>
    <% end %>
    <%= form_tag("/mypage/leader/deny/#{r.id}", method: "GET") do %>
      <button class="btn btn-primary-outline waves-effect btn-size" type="submit">미안해요~</button>
    <% end %>
    <hr>
  <% end %>
<% end %>
