<div class="container">

<!-- PORTFOLIO -->

  <div id="wrapper-container" style="margin-top: 120px;">
  
    <div class="container object">
    
      <div class="work">
        <%= form_tag("/teambuilding_create/create_process", method: "POST", multipart: true) do%>
        	<figure class="white">
          	<input type="button" id="file_upload_button"/>
          	<input type="file" name="picture" id="file_upload"/>
            <div id="wrapper-part-info">
              <div class="part-info-image">
                <img src="/img/mockup-1.jpg" alt="" width="628" height="437"/>
                <img src="/img/mockup-2.jpg" alt="" width="628" height="437"/>
                <img src="/img/mockup-3.jpg" alt="" width="628" height="437"/>
              </div>
            </div>
          </figure>	

          <div class="wrapper-text-description side-info">
            <label for="team_name">Team Name</label>
          	<input type="text" id="team_name" name="name" class="text-file"/>
          	<input type="text" name="abstract" class="text-file"/>
          	<input type="text" name="number" class="text-file"/>
          	<input type="text" name="tag1" class="text-file"/>
          	<input type="text" name="tag2" class="text-file"/>
          	<input type="text" name="tag3" class="text-file"/>
          	<input type="text" name="tag4" class="text-file"/>
          	<input type="text" name="tag5" class="text-file"/>
              
            <div class="wrapper-morefrom">
            	<div class="text-morefrom">More from .psd</div>
              <div class="image-morefrom">
            	  <a href="#"><div class="image-morefrom-1"><img src="/img/psd-1.jpg" alt="" width="430" height="330"/></div></a>
                <a href="#"><div class="image-morefrom-2"><img src="/img/psd-2.jpg" alt="" width="430" height="330"/></div></a>
                <a href="#"><div class="image-morefrom-3"><img src="/img/psd-3.jpg" alt="" width="430" height="330"/></div></a>
                <a href="#"><div class="image-morefrom-4"><img src="/img/psd-6.jpg" alt="" width="430" height="330"/></div></a>
              </div>
            </div>
            
          </div> <!-- wrapper-text-description -->
        <% end %>
			</div> <!-- work -->
		</div>
  </div>      
</div> <!-- container -->